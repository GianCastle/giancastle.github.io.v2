{
    "extends": "react-app",
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "indent": ["error", 2],
      "max-len": ["warn", 100],
      "semi": ["off"],
      "react-hooks/rules-of-hooks": "error", // Checks rules of Hooks
      "react-hooks/exhaustive-deps": "warn"
    }
  }
  